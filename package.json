{
  "name": "generator-mistery",
  "version": "0.0.2",
  "description": "Simple scaffolding for Typescript project.",
  "scripts": {
    "eslint": "eslint generators --cache -c .config/.eslintrc.yml --ext js",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check generators --config .config/.prettierrc.json",
    "prettier-write": "prettier --write generators --config .config/.prettierrc.json",
    "test-only": "npm run eslint && npm run prettier"
  },
  "files": [
    "generators/app"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santimirandarp/generator-mistery.git"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Santi Miranda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/santimirandarp/generator-mistery/issues"
  },
  "homepage": "https://github.com/santimirandarp/generator-mistery#readme",
  "dependencies": {
    "camelcase": "^7.0.1",
    "yeoman-generator": "^5.9.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/yeoman-generator": "^5.2.11",
    "eslint": "^8.41.0",
    "eslint-config-cheminfo-typescript": "^11.3.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "vitest": "^0.31.1"
  }
}
