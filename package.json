{
  "name": "generator-simple",
  "version": "0.0.0",
  "description": "Simple scaffolding for Typescript project.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
